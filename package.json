  "scripts": {
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build": "tsc && vite build",
    "build-storybook": "pnpm storybook:build",
    "build:netlify": "netlify build",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy -p",
    "dev": "vite",
    "dev:https": "vite --https",
    "dev:live": "netlify dev --live",
    "dev:netlify": "netlify dev",
    "dev:open": "vite --open",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "format": "eslint src --fix --report-unused-disable-directives && stylelint './src/**/*.{css,scss}' --fix",
    "format:all": "pnpm format && prettier --write './**/*.{json,yaml,yml,ts,js,cjs,md}'",
    "init:netlify": "netlify init",
    "lint": "eslint src --report-unused-disable-directives && stylelint './src/**/*.{css,scss}'",
    "prepare": "husky",
    "preview": "vite preview --host",
    "start": "pnpm dev",
    "storybook": "export SET DISABLE_CSS_TYPE_GEN=true && storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:preview": "serve storybook-static",
    "storybook:smoke-test": "storybook dev --ci --smoke-test",
    "storybook:test": "test-storybook",
    "chromatic": "chromatic --exit-zero-on-changes",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "type-coverage": "type-coverage"
  },
  "lint-staged": {
    "./src/**/*.tsx": [
      "eslint --fix --report-unused-disable-directives",
      "eslint"
    ],
    "./src/**/*.{css,scss}": [
      "stylelint --fix",
      "stylelint"
    ],
    "./**/*.md": "prettier --write",
    "./**/*.{ts,js,cjs}": "prettier --write"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
