{
  "name": "timjames.dev",
  "version": "0.0.0",
  "description": "Personal site for Tim W James - Portfolio, Blog, and more :rocket:",
  "author": {
    "name": "Tim W James",
    "email": "tim.jameswork9800@gmail.com",
    "url": "https://timjames.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/Tim-W-James/timjames.dev#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tim-W-James/timjames.dev.git"
  },
  "keywords": [
    "Tim-W-James, Portfolio, Blog"
  ],
  "bugs": {
    "url": "https://github.com/Tim-W-James/timjames.dev/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.29.1",
  "main": "src/main.ts",
  "private": "true",
  "scripts": {
    "prepare": "husky install",
    "init:netlify": "netlify init",
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "dev:live": "netlify dev --live",
    "dev:open": "vite --open",
    "dev:https": "vite --https",
    "start": "pnpm dev",
    "build": "tsc && vite build",
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build:netlify": "netlify build",
    "preview": "vite preview --host",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy -p",
    "lint": "eslint src && stylelint './src/**/*.{css,scss}'",
    "format": "eslint src --fix && stylelint './src/**/*.{css,scss}' --fix",
    "format:all": "pnpm format && prettier --write './**/*.{json,yaml,yml,ts,js,cjs,md}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "storybook": "export SET DISABLE_CSS_TYPE_GEN=true && storybook dev -p 6006 --no-open",
    "storybook:test": "test-storybook",
    "storybook:build": "storybook build",
    "build-storybook": "pnpm storybook:build",
    "storybook:preview": "serve storybook-static",
    "storybook:smoke-test": "storybook dev --ci --smoke-test",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "chromatic": "chromatic --exit-zero-on-changes",
    "type-coverage": "type-coverage"
  },
  "lint-staged": {
    "./src/**/*.tsx": [
      "eslint --fix",
      "eslint"
    ],
    "./src/**/*.{css,scss}": [
      "stylelint --fix",
      "stylelint"
    ],
    "./**/*.md": "prettier --write",
    "./**/*.{json,yaml,yml,ts,js,cjs}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
