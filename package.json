{
  "name": "tim-w-james-site",
  "version": "0.0.0",
  "description": "Personal site for Tim W James - Portfolio, Blog, and more :rocket:",
  "author": {
    "name": "Tim W James",
    "email": "tim.jameswork9800@gmail.com",
    "url": "https://timjames.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/Tim-W-James/site#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tim-W-James/site.git"
  },
  "keywords": [
    "Tim-W-James, Portfolio, Blog"
  ],
  "bugs": {
    "url": "https://github.com/Tim-W-James/site/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.5.1",
  "main": "src/main.ts",
  "private": "true",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "dev:live": "netlify dev --live",
    "dev:open": "vite --open",
    "dev:https": "vite --https",
    "start": "pnpm dev",
    "build": "tsc && vite build",
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build:netlify": "netlify build",
    "preview": "vite preview --host",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy -p",
    "lint": "eslint src && stylelint './src/**/*.{css,scss}'",
    "format": "eslint src --fix && stylelint './src/**/*.{css,scss}' --fix",
    "format:all": "pnpm format && prettier --write './**/*.{json,yaml,yml,ts,js,cjs,md}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "preview-storybook": "http-server storybook-static"
  },
  "lint-staged": {
    "./src/**/*.tsx": [
      "eslint --fix",
      "eslint"
    ],
    "./src/**/*.{css,scss}": [
      "stylelint --fix",
      "stylelint"
    ],
    "./**/*.md": "prettier --write",
    "./**/*.{json,yaml,yml,ts,js,cjs}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/types": "^7.18.7",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-backgrounds": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-measure": "^6.5.9",
    "@storybook/addon-outline": "^6.5.9",
    "@storybook/addon-toolbars": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/channel-postmessage": "^6.5.9",
    "@storybook/channel-websocket": "^6.5.9",
    "@storybook/client-api": "^6.5.9",
    "@storybook/preview-web": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^17.0.38",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.17.0",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "debug": "^4.3.4",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-vitest": "^0.0.8",
    "happy-dom": "^6.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.1",
    "netlify-cli": "^10.6.2",
    "pnpm": "^7.5.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.52.3",
    "storybook-addon-react-router-v6": "^0.1.10",
    "storybook_vitest_addon": "^0.0.8",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-css-modules-no-global-scoped-selector": "^1.0.2",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "unplugin-auto-import": "^0.9.0",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.17.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "readonly-types": "^3.0.0",
    "source-map-support": "^0.5.21",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
